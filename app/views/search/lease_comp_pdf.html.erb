              <p>Suite </p>
              <p>City</p>
              <p>State </p>
              <p>Zip Code </p>
              <p>Submarket</p>

            </div>
            <div class="col-md-3 ls-table-box">

              <h1 class="table-hadding-mrg"></h1>

              <p>&nbsp;<%=@comp_record.property_name.to_s.capitalize!%> </p>
              <p>&nbsp;<%= @comp_record.property_type.to_s.capitalize! %> </p>
              <p>&nbsp;<%=@comp_record.class_type.to_s.capitalize!%></p>


              <h1 class="table-hadding-mrg"></h1>

              <p>&nbsp;<%=@comp_record.address1%></p>
              <p>&nbsp;<%= @comp_record.suite %> </p>
              <p>&nbsp;<%=@comp_record.city%></p>
              <p>&nbsp;<%=@comp_record.state%> </p>
              <p>&nbsp;<%=@comp_record.zipcode%></p>
              <p>&nbsp;<%=@comp_record.submarket%></p>


            </div>
          </div>

