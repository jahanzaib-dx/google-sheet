            <td><p class="comp-table-pre">Submarket</p></td>
            <td><p class="comp-table-pre"><%=@comp_record.submarket%> </p></td>

          </tr>

          <tr>
            <td><p class="comp-table-pre">Deal Type</p></td>
            <td><p class="comp-table-pre"><%=@comp_record.deal_type%></p></td>
            <td><p class="comp-table-pre"></p></td>
            <td><p class="comp-table-pre"> </p></td>

          </tr>
          
          <tr>
            <td><p class="comp-table-pre">&nbsp;</p></td>
            <td><p class="comp-table-pre"></p></td>
            <td><p class="comp-table-pre"></p></td>
            <td><p class="comp-table-pre"> </p></td>

          </tr>

