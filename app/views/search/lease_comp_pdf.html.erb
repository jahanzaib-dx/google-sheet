<div class="container">
  <div class="comp-width-container">
    <div class="col-md-12 comp-background-container ">
      <div class="comp-detail-container">
        <div class="col-md-12 successfully-added-main">
          <div class="col-md-4">
            <!--<h3 class="successfully-hadding">Successfully added: </h3>-->
            <h2 class="broadway-hadding"><%= @comp_record.address1 %></h2>
          </div>
          <div class="col-md-8 pull-right">
            <p class="custom-pdf-btn"><a href="#!"> Create Custom PDF</a></p>
            <p class="creat-pdf-btn"><a href="<%=search_lease_comp_pdf_path(@comp_record.id)%>" target="_blank"> Create  PDF</a></p>
            <!--<p class="edit-record-btn"><a href="#!">Edit Record</a></p>-->
          </div>
        </div>
      </div>
      <div class="adress-bar-main-container">
        <div class="col-md-12 adress-bar-container">
          <div class="adress-detail">
            <h1 class="nespresso"><%=@comp_record.contact%></h1>
            <h3 class="adress-content"><%= @comp_record.address1 %><br/>
              <%= @comp_record.suite %><br/>
              <%= @comp_record.city %>, <%= @comp_record.state %> <%= @comp_record.zipcode %></h3>
          </div>
          <img src="<%= image_url 'adress-img.jpg' %>" class="adress-img">
        </div>
      </div>



      <div class="comp-table-container">
        <h1 class="detail-comp-hadding">Comp Detail</h1>
        <div class="row">
          <div class="table-box">

            <div class="col-md-3 ls-table-box">
              <h1>Lease</h1>

              <p>Tenant Name    </p>
              <p>Industry </p>
              <p>Size (SF) </p>
              <p>Base Rent ($/SF)</p>

              <p>Escalation (%) </p>
              <p>Lease Structure   </p>
              <p>Commencement Date</p>
              <p>Term (Months) </p>
              <p>Free Rent (Months) </p>
              <p>Tenant Improvements ($/SF)</p>
              <p>Additional Concessions ($/SF)</p>
              <p>Costs to Tenant ($/SF)</p>
              <p>Lease Type</p>
            </div>
            <div class="col-md-3 ls-table-box">
              <h1>&nbsp;</h1>

              <p>&nbsp;<%=@comp_record.contact%></p>
              <p>&nbsp;<%=@comp_record.industry_type%></p>
              <p>&nbsp;<%=@comp_record.size%></p>
              <p>&nbsp;<%= number_to_currency(@comp_record.base_rent.to_f, {:precision=>2})%></p>
              <p>&nbsp;<%=@comp_record.escalation%></p>
              <p>&nbsp;<%=@comp_record.data['leasestructure_name']%></p>
              <p>&nbsp;<%=@comp_record.lease_commencement_date.strftime('%m/%d/%Y')%></p>
              <p>&nbsp;<%=@comp_record.lease_term_months%></p>
              <p>&nbsp;<%=@comp_record.free_rent%></p>
              <p>&nbsp;<%= number_to_currency(@comp_record.tenant_improvement.to_f, {:precision=>2})%></p>
              <p>&nbsp;<%= number_to_currency(@comp_record.landlord_concessions_per_sf.to_f, {:precision=>2})%></p>
              <p>&nbsp;<%=@comp_record.lease_commencement_date.strftime('%m/%d/%Y')%></p>
              <p>&nbsp;<%=@comp_record.lease_type%></p>








            </div>
            <div class="col-md-3 ls-table-box">
              <h1>Building</h1>

              <p>Property Name </p>
              <p>Property Type </p>
              <p>Class Type</p>

              <h1 class="table-hadding-mrg">Location</h1>

