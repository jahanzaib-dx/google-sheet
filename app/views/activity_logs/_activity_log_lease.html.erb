<tr>
  <td height="51" class="result_lock_icon">
    <% if activity_log.receiver_id == current_user.id %>
    
    	<% if activity_log.status=='Rejected' %>
    		<a href="#!" class="set_aid" data-aid="<%= activity_log.id%>" data-status="Unlock" data-toggle="modal"  data-target="#Modaloptions2" >Declined</a>
    	<% elsif activity_log.status=='full_owner' %>
    		Approved
    	<% else %>
    		<a href="#!" class="set_aid" data-aid="<%= activity_log.id%>" data-status="Unlock" data-toggle="modal"  data-target="#Modaloptions1" >Approved</a>
    	<% end %>
    	
        <%##= "#{ activity_log.status=='Rejected'? 'Declined':'Approved' }" %>
    <% else %>
    	<%=
           case activity_log.status          
           when 'full'
             image_tag("lock-un.png" , :class => "unlock_img")
           when 'full_owner'
             image_tag("lock-un.png" , :class => "unlock_img")
           when 'partial'
             #image_tag("un-lock.png" , :class => "partial_img");
             image_tag("lock-b.png" , :class => "partial_img")
           when 'Rejected'
             "Declined"
           end
        %>
    <% end %>

  </td>
  <td class="result_line border_full"><p><%= activity_log.created_at.strftime('%m/%d/%Y') %></p></td>
  <td class="result_line border_full"><p><%= activity_log.tenant_record.user.name %></p></td>
  <td class="result_line img_width">
  	<p> 
  	<a href="http://www.google.com/maps?cbll=<%= activity_log.tenant_record.latitude%>,<%= activity_log.tenant_record.longitude%>&layer=c" target="_blank">
      <div class="" style="float:right; height:40px; width: 40px; background: #edeced url('http://maps.googleapis.com/maps/api/streetview?size=50x50&location=<%= activity_log.tenant_record.address1%>+<%= activity_log.tenant_record.city%>+<%= activity_log.tenant_record.state%>+<%= activity_log.tenant_record.zipcode%>') no-repeat center;">
      </div>
    </a>
            <%##= image_tag("tbl-img.jpg")%> 
            
  </p></td>
  <td class="result_line"><p><%= "#{activity_log.tenant_record.complete_address}"%></p></td>
  <td class="result_line"><p><%= activity_log.tenant_record.company %></p></td>
  <td class="result_line"><p><%= activity_log.tenant_record.base_rent %></p></td>
  <td class="result_line"><p><%= activity_log.tenant_record.sizerange %></p></td>
  <td class="result_line"><p><%= activity_log.tenant_record.lease_commencement_date.year %></p></td>
  

</tr>